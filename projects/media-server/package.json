    "awaitqueue": "^2.3.3",
    "axios": "^0.21.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "debug": "^4.3.1",
    "fastify": "^4.17.0",
    "helmet": "^5.0.2",
    "internal-ip": "^6.2.0",
    "ioredis": "5.0.4",
    "mediasoup": "3.14.8",
    "mysql": "^2.18.1",
    "nacos": "2.6.0",
    "nestjs-axios": "workspace:^1.0.0",
    "nestjs-otel": "^6.1.1",
    "nestjs-pino": "^4.1.0",
    "nestjs-redis": "workspace:^1.0.0",
    "node-disk-info": "^1.3.0",
    "opentelemetry-node-metrics": "^3.0.0",
    "pino": "^9.2.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.2",
    "protoo-server": "^4.0.5",
    "public-ip": "^6.0.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "7.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "websocket": "^1.0.33"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/node": "^20.14.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "pino-elasticsearch": "6.7.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^5.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
