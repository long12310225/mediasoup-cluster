{
  "name": "media-server",
  "version": "0.1.0",
  "description": "",
  "author": "michael",
  "scripts": {
    "dev": "NODE_ENV=dev cross-env PORT=4443 SLAVE_FOR=master nest start --watch",
    "dev:producer": "NODE_ENV=dev cross-env PORT=3021 SLAVE_FOR=producer nest start --watch",
    "dev:consumer": "NODE_ENV=dev cross-env PORT=3031 SLAVE_FOR=consumer nest start --watch",
    "dev:consumer2": "NODE_ENV=dev cross-env PORT=3032 SLAVE_FOR=consumer nest start --watch",
    "dev:all": "sh sh/run/run.sh dev",
    "debug": "NODE_ENV=dev cross-env PORT=4443 SLAVE_FOR=master nest start --debug --watch",
    "debug:producer": "NODE_ENV=dev cross-env PORT=3011 SLAVE_FOR=producer nest start --debug --watch",
    "debug:consumer": "NODE_ENV=dev cross-env PORT=3012 SLAVE_FOR=consumer nest start --debug --watch",
    "qa:main": "sh sh/run/run.sh qa_main",
    "qa:producer": "sh sh/run/run.sh qa_producer",
    "qa:consumer": "sh sh/run/run.sh qa_consumer",
    "prod": "NODE_ENV=prod node dist/main",
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@nestjs/platform-fastify": "^10.3.9",
    "@nestjs/typeorm": "^10.0.2",
    "@opentelemetry/api": "1.8.0",
    "@opentelemetry/auto-instrumentations-node": "0.46.0",
    "@opentelemetry/context-async-hooks": "1.24.1",
    "@opentelemetry/core": "^1.24.1",
    "@opentelemetry/exporter-prometheus": "0.51.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.1",
    "@opentelemetry/host-metrics": "^0.35.1",
    "@opentelemetry/instrumentation-nestjs-core": "0.37.1",
    "@opentelemetry/instrumentation-fastify": "0.32.0",
    "@opentelemetry/instrumentation-http": "0.35.0",
    "@opentelemetry/instrumentation-ioredis": "^0.43.0",
    "@opentelemetry/instrumentation-mysql": "^0.41.0",
    "@opentelemetry/propagator-b3": "1.24.1",
    "@opentelemetry/sdk-node": "0.51.1",
    "@opentelemetry/sdk-trace-base": "1.24.1",
    "@opentelemetry/exporter-jaeger": "1.9.0",
    "@opentelemetry/resources": "1.9.0",
    "@opentelemetry/semantic-conventions": "1.9.0",
