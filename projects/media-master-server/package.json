{
  "name": "media-master-server",
  "version": "0.0.1",
  "description": "",
  "author": "michael",
  "scripts": {
    "dev": "NODE_ENV=dev cross-env PORT=4443 SLAVE_FOR=master nest start --watch",
    "dev:producer": "NODE_ENV=dev cross-env PORT=3011 SLAVE_FOR=producer nest start --watch",
    "dev:consumer": "NODE_ENV=dev cross-env PORT=3012 SLAVE_FOR=consumer nest start --watch",
    "qa": "NODE_ENV=qa nest start --watch",
    "prod": "NODE_ENV=prod node dist/main",
    "debug": "nest start --debug --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@nestjs/platform-fastify": "^10.3.9",
    "@nestjs/typeorm": "^10.0.2",
    "@sitespeed.io/throttle": "^0.5.4",
    "awaitqueue": "^2.3.3",
    "axios": "^0.21.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^4.17.0",
    "helmet": "^5.0.2",
    "internal-ip": "^6.2.0",
    "mediasoup": "3.12.3",
    "nacos": "2.6.0",
    "node-disk-info": "^1.3.0",
    "node-fetch": "^3.3.1",
    "pg": "^8.12.0",
    "protoo-server": "^4.0.5",
    "public-ip": "^6.0.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.20",
    "websocket": "^1.0.33",
    "debug": "^4.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/node": "^20.14.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^5.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
